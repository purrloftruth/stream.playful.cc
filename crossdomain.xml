<?xml version="1.0" encoding="UTF-8"?>
<cross-domain-policy xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:noNamespaceSchemaLocation="http://www.adobe.com/xml/schemas/PolicyFile.xsd">
    <allow-access-from domain="*.playful.cc" secure="false" />
    <allow-access-from domain="www.playful.cc" secure="false" />
    <allow-access-from domain="localhost" secure="false" />
    <allow-access-from domain="127.0.0.1" secure="false" />
    <allow-http-request-headers-from domain="*.playful.cc" headers="*" secure="false" />
    <allow-http-request-headers-from domain="127.0.0.1" headers="*" secure="false" />
    <allow-http-request-headers-from domain="localhost" headers="*" secure="false" />
</cross-domain-policy>
